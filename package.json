{
  "name": "wikipedia_viewer",
  "version": "1.0.0",
  "description": "Web application to search articles.",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.min.css --out-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fjollsfinn/Wikipedia_Viewer.git"
  },
  "keywords": [
    "Wikipedia"
  ],
  "author": "Bart≈Çomiej Tuchowski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fjollsfinn/Wikipedia_Viewer/issues"
  },
  "homepage": "https://github.com/Fjollsfinn/Wikipedia_Viewer#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "node-sass": "^4.8.3",
    "postcss-cli": "^10.1.0"
  }
}
